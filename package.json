  "engines": {
    "node": "13.5.0",
    "npm": "6.13.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterdee/ts-classes"
  },
  "keywords": [
    "Example",
    "Module",
    "NPM",
    "Typescript"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  }
